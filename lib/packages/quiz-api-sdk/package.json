{
  "name": "@tanacchi/quiz-api-sdk",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Quiz API SDK with type safety and MSW support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json && orval --config orval.config.ts && tsx scripts/gen-validators.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "generate": "orval --config orval.config.ts",
    "gen:msw": "tsx scripts/gen-msw-handlers.ts"
  },
  "keywords": ["quiz", "api", "sdk", "typescript"],
  "author": "tanacchi",
  "license": "MIT",
  "packageManager": "pnpm@10.14.0",
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@tsconfig/strictest": "^2.0.5",
    "@types/node": "^20.19.10",
    "msw": "^2.10.4",
    "orval": "^7.11.2",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "zod": "^4.0.17"
  },
  "dependencies": {
    "neverthrow": "^8.2.0"
  }
}
